{
  "name": "node-shared",
  "version": "2.0.1",
  "description": "Shared logic for Node.JS",
  "repository": {
    "url": "https://github.com/betula/node-provide"
  },
  "source": "./index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/index.ts",
    "/jest-cleanup-after-each.js",
    "/README.md"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest --coverage",
    "test-with-coveralls": "npx jest --coverage && cat ./coverage/lcov.info | coveralls",
    "eslint": "npx eslint index.ts",
    "prepare": "npm run eslint && npm run build && npm run test"
  },
  "keywords": [
    "di",
    "dependency injection",
    "dependency provider",
    "provide",
    "injection",
    "inject",
    "shared",
    "shared logic",
    "isolate",
    "node"
  ],
  "author": "Slava Birch <mail@betula.co>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "typescript": "^3.7.5"
  }
}
