{
  "name": "node-provide",
  "version": "1.0.0",
  "description": "Async context based Dependency Injection for Node.JS",
  "repository": {
    "url": "https://github.com/betula/node-provide"
  },
  "source": "./index.ts",
  "main": "./release/index.js",
  "types": "./release/index.d.ts",
  "files": [
    "/release",
    "/index.ts",
    "/jest-cleanup-after-each.js",
    "/README.md"
  ],
  "engines" : {
    "node" : ">=8.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest --coverage",
    "test-with-coveralls": "npx jest --coverage && cat ./coverage/lcov.info | coveralls",
    "eslint": "npx eslint index.ts",
    "prepare": "npm run eslint && npm run build && npm run test"
  },
  "keywords": [
    "di",
    "dependency injection",
    "dependency provider",
    "provide",
    "injection",
    "inject",
    "zone",
    "node"
  ],
  "author": "Viacheslav Bereza <mail@betula.co>",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}
