{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "outDir": ".dist",
    "baseUrl": ".",
    "paths": {
      "@services/*": [
        "services/*"
      ]
    }
  },
  "exclude": [
    "./**/*.js"
  ]
}
