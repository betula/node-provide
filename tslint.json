{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [
        true,
        "double"
      ],
      "variable-name": false,
      "ter-arrow-parens": [true, "always"]
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "**/*.js"
      ]
  }
}
