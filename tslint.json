{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double"
    ],
    "variable-name": false,
    "ter-arrow-parens": [true, "always"],
    "max-line-length": false,
    "no-this-assignment": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/*.js"
    ]
  }
}
